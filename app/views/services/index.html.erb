<h1>All your services</h1>

<% if @services.empty? %>
  <p>You don't have any service yet</p>
<% else %>
  <ul>
    <% @services.each do |service| %>
      <li><%= link_to service.title, service_path(service) %></li>
    <% end %>
  </ul>
<% end %>
<%= link_to "Create new service", new_service_path %>